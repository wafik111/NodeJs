<%include ../partials/header%>

        <h3> events List </h3>
        <table class="table table-dark">
                <thead>
                  <tr>
                    <!-- <th scope="col">#</th> -->
                    <th scope="col">Title</th>
                    <th scope="col">MainSpeaker</th>
                    <th scope="col">OtherSpeakers</th>

                  </tr>
                </thead>
                <tbody>
                 <%events.forEach((event)=>{%>
                    <tr>
                      <!-- <td ><%=event._id%></td> -->
                      <td><%=event.title%></td>
                      <% if (event.mainSpeaker){%>
                        <td><%=event.mainSpeaker.name%></td>

                      <%}%>
                      <td>
                        <ul>
                          <%event.otherSpeakers.forEach((otherSpeaker)=>{%>
                              <li><%=otherSpeaker.name%></li>
                          <%})%>
                        </ul>
                      </td>
                      <td><a href="/events/edit/<%=event._id%>" class="btn btn-primary">Edit</a></td>
                      <td><button class="btn btn-danger" id="<%=event._id%>">Delete</button></td>


                    </tr>
                 <%})%>
                 
                </tbody>
              </table>
              <script src="/js/delete.js"></script>
              <%include ../partials/footer%>

