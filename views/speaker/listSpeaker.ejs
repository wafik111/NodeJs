<%include ../partials/header%>

        <h3> Speakers List </h3>
        <table class="table table-dark">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">name</th>
                    <th scope="col">Age</th>
                  </tr>
                </thead>
                <tbody>
                 <%speakers.forEach((speaker)=>{%>
                    <tr>
                      <td><%=speaker._id%></td>
                      <td><%=speaker.name%></td>
                      <td><%=speaker.age%></td>
                      <td><a href="/speaker/edit/<%=speaker._id%>" class="btn btn-primary">Edit</a></td>
                      <td><button class="btn btn-danger" id="<%=speaker._id%>">Delete</button></td>
                    </tr>
                 <%})%>
                </tbody>
              </table>
              <script src="/js/deleteSpeaker.js"></script>

<%include ../partials/footer%>